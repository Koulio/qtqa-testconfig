PULSE_BASE_DIR=BASE_DIR
QTQA_SOURCE_PROPERTY='string from source property'
QTQA_USING_NONEXISTENT_PROPERTY='this should be empty: ""'
QTQA_USING_NONEXISTENT_PROPERTY_WITH_BACKSLASH='this should be empty: "\foo\bar"'
QTQA_USING_PROPERTY='This property uses another property: "string from source property"'
export PULSE_BASE_DIR QTQA_SOURCE_PROPERTY QTQA_USING_NONEXISTENT_PROPERTY QTQA_USING_NONEXISTENT_PROPERTY_WITH_BACKSLASH QTQA_USING_PROPERTY
testscript
